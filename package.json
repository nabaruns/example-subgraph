{
  "name": "cosmos-event-example",
  "version": "0.1.0",
  "repository": "https://github.com/graphprotocol/example-subgraph",
  "license": "MIT",
  "scripts": {
    "prepare:cosmoshub": "mustache config/cosmoshub.json subgraph.template.yaml > subgraph.yaml",
    "prepare:osmosis": "mustache config/osmosis.json subgraph.template.yaml > subgraph.yaml",
    "codegen": "graph codegen",
    "build": "graph build",
    "create-local": "graph create cosmos-event-example --node http://localhost:8020",
    "deploy-local": "graph deploy cosmos-event-example -l v0.1.0 --ipfs http://localhost:5001 --node http://localhost:8020",
    "remove-local": "graph remove cosmos-event-example --node http://localhost:8020",
    "create": "graph create cosmos-event-example --node https://api.thegraph.com/deploy/",
    "deploy": "graph deploy cosmos-event-example --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.30.0",
    "@graphprotocol/graph-ts": "^0.27.0",
    "mustache": "^4.2.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0"
  }
}
